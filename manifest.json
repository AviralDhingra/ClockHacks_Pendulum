{
  "manifest_version": 3,
  "name": "Pendulum ",
  "version": "0.0.1",
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "128": "clock_icon.png" },
  "action": {
    "default_popup": "Popup.html",
    "default_title": "Website Blocker"
  }
}
